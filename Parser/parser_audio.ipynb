import os
#in future make this script more function friendly
#input file u want parsed 

file = open(r"C:\Users\kmymi\OneDrive\Desktop\440Hz DATA.txt")
out = open("Parsed.txt","w+")

#parsed out audio and time stamps 
Lines = file.readlines()
count = 1
for line in Lines: 
    #print(count)
    #print(line)
    if count > 5:
        count = 1
    if count == 5:
        line = line.replace(" ","") 
        line = line.rstrip("\n")
        #write audio data to file, deleting spaces and new lines
        out.write(line)
    count += 1
out.close()
file.close()

