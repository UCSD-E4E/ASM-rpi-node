############################################################################
#calculating number of packets in the file
out = open("Parsed.txt","r")
unparsed_packetsize = 2158 
length_file = len(out.read())
num_packets = int(length_file / unparsed_packetsize)
out.close()

#############################################################################
#parsing the packets for purely data 
out = open("Parsed.txt","r")
final = open("Fully-Parsed.txt","w+") #in future save as timestamp
for x in range(num_packets):
    header = out.read(105)
    data = out.read(2048)
    final.write(data)
    end_check = out.read(5)
out.close()
final.close()

